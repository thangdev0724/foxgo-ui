{
  "name": "foxgo-ui",
  "version": "0.1.0",
  "description": "FoxGo UI - A Vue 3 component library with PrimeVue integration",
  "author": "FoxGo Team",
  "license": "MIT",
  "type": "module",
  "main": "./dist/foxgo-ui.umd.js",
  "module": "./dist/foxgo-ui.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/foxgo-ui.es.js",
      "require": "./dist/foxgo-ui.umd.js"
    },
    "./style.css": "./dist/foxgo-ui.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui-components",
    "primevue",
    "typescript",
    "scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thangdev0724/foxgo-ui.git"
  },
  "homepage": "https://github.com/thangdev0724/foxgo-ui#readme",
  "bugs": {
    "url": "https://github.com/thangdev0724/foxgo-ui/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "peerDependenciesMeta": {
    "primevue": {
      "optional": true
    },
    "@primevue/themes": {
      "optional": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@primevue/themes": "^4.5.0",
    "@storybook/addon-a11y": "^10.1.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-onboarding": "^10.1.0",
    "@storybook/addon-vitest": "^10.1.0",
    "@storybook/vue3-vite": "^10.1.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^10.1.0",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "pinia": "^3.0.4",
    "playwright": "^1.57.0",
    "prettier": "3.6.2",
    "primevue": "^4.5.0",
    "sass": "^1.94.2",
    "storybook": "^10.1.0",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.14",
    "vue": "^3.5.25",
    "vue-tsc": "^3.1.5"
  }
}
